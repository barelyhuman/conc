{
  "name": "@barelyreaper/conch",
  "description": "micro library for batch running promises",
  "version": "0.0.1",
  "main": "dist/index.cjs.js",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "repository": "git@github.com:barelyhuman/conch",
  "author": "Reaper Gelera <sidgroks@gmail.com>",
  "bugs": {
    "url": "https://github.com/barelyhuman/conch/issues"
  },
  "homepage": "https://github.com/barelyhuman/conch",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "batch promise",
    "promises",
    "concurrency",
    "batch"
  ],
  "scripts": {
    "prepare": "npm run build",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prebuild": "npm run clean",
    "clean": "npx rimraf dist",
    "fix": "standard --fix || true",
    "test": "node test.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "bundt": "^1.1.2",
    "husky": "=4",
    "lint-staged": ">=10",
    "rimraf": "^3.0.2",
    "rollup": "^2.41.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "standard": "^16.0.3"
  },
  "lint-staged": {
    "*.{js,css}": "npm run fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}